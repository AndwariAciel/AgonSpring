<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0"
      prefWidth="500.0" alignment="CENTER" fx:id="box"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.andwari.agon.app.controller.views.MatchItemController">

    <Label fx:id="status" alignment="CENTER" prefWidth="60.0"
           text="Playing" textFill="GREY">
        <font>
            <Font name="System Italic" size="12.0" />
        </font>
    </Label>
    <Label fx:id="player1" alignment="CENTER" prefWidth="190.0"
           text="Player1">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label fx:id="scoreP1" alignment="CENTER"
           contentDisplay="CENTER" prefWidth="25.0" text="-"
           textAlignment="CENTER">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label alignment="CENTER" prefWidth="10.0" text=":">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label fx:id="scoreP2" alignment="CENTER" prefWidth="25.0"
           text="-">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label fx:id="player2" alignment="CENTER" prefWidth="190.0"
           text="Player2">
        <font>
            <Font size="18.0" />
        </font>
    </Label>

</HBox>
